{
  "name": "monishroy/visitor-tracking",
  "description": "A Laravel package for tracking website visitors",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Monish Roy",
      "email": "monishroy87@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "MonishRoy\\VisitorTracking\\": "src/"
    },
    "files": [
      "src/Helpers/Visitor.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "MonishRoy\\VisitorTracking\\VisitorTrackingServiceProvider"
      ]
